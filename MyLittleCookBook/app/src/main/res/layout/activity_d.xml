<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Rozoviy"
    tools:context=".d">

    <Button
        android:id="@+id/button6"
        android:layout_width="141dp"
        android:layout_height="65dp"
        android:text="Назад"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="304dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Конфеты из овсяных хлопьев, орехов, изюма и фиников"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="274dp"
        android:layout_height="46dp"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="284dp"
        android:text="Хлопья овсяные - 100 г"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="274dp"
        android:layout_height="46dp"
        android:layout_marginEnd="85dp"
        android:layout_marginBottom="330dp"
        android:text="Какао-порошок - 10 г"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="274dp"
        android:layout_height="46dp"
        android:layout_marginEnd="85dp"
        android:text="Орехи грецкие (ядра) - 30 г"
        app:layout_constraintBottom_toTopOf="@+id/textView30"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="274dp"
        android:layout_height="46dp"
        android:layout_marginEnd="85dp"
        android:text="Изюм - 50 г"
        app:layout_constraintBottom_toTopOf="@+id/textView31"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="274dp"
        android:layout_height="46dp"
        android:layout_marginEnd="85dp"
        android:text="Финики - 80 г"
        app:layout_constraintBottom_toTopOf="@+id/textView32"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>