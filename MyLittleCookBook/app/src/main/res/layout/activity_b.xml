<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Rozoviy"
    tools:context=".b" >

    <Button
        android:id="@+id/button"
        android:layout_width="141dp"
        android:layout_height="65dp"
        android:layout_marginStart="270dp"
        android:text="Назад"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="251dp"
        android:layout_height="35dp"
        android:layout_marginTop="36dp"
        android:gravity="center"
        android:text="Маки-суши"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:layout_marginEnd="76dp"
        android:text="Уксус рисовый - 50 мл"
        app:layout_constraintBottom_toTopOf="@+id/textView18"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="140dp"
        android:text="Рис круглозерный (для суши) - 0,5 кг"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:layout_marginEnd="76dp"
        android:text="Сахар - 3 ст. ложки"
        app:layout_constraintBottom_toTopOf="@+id/textView19"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:layout_marginEnd="76dp"
        android:text="Соль - 1 ч. ложка"
        app:layout_constraintBottom_toTopOf="@+id/textView20"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="184dp"
        android:text="Вода - 550 мл"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:layout_marginEnd="76dp"
        android:text="Нори - 10 листов"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:layout_marginEnd="76dp"
        android:text="Лосось свежий - 150 г"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="76dp"
        android:text="Тунец свежий - 150 г"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="76dp"
        android:text="Огурец свежий - 1 шт."
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>