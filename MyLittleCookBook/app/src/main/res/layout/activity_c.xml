<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Rozoviy"
    tools:context=".c">

    <Button
        android:id="@+id/button5"
        android:layout_width="141dp"
        android:layout_height="65dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="254dp"
        android:layout_height="32dp"
        android:layout_marginTop="36dp"
        android:gravity="center"
        android:text="Свекольный салат"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="296dp"
        android:layout_height="42dp"
        android:layout_marginEnd="81dp"
        android:text="Сок лимонный"
        app:layout_constraintBottom_toTopOf="@+id/textView27"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="296dp"
        android:layout_height="42dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="264dp"
        android:text="Соль мелкая"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="296dp"
        android:layout_height="42dp"
        android:layout_marginEnd="81dp"
        android:text="Горошек зеленый консервированный"
        app:layout_constraintBottom_toTopOf="@+id/textView24"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="296dp"
        android:layout_height="42dp"
        android:layout_marginEnd="81dp"
        android:text="Масло подсолнечное"
        app:layout_constraintBottom_toTopOf="@+id/textView22"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="296dp"
        android:layout_height="42dp"
        android:layout_marginEnd="81dp"
        android:text="Огурцы солёные"
        app:layout_constraintBottom_toTopOf="@+id/textView23"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="296dp"
        android:layout_height="42dp"
        android:layout_marginEnd="81dp"
        android:text="Свекла"
        app:layout_constraintBottom_toTopOf="@+id/textView25"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>